{
  "name": "magajico-sports-platform",
  "version": "2.0.0",
  "private": true,
  "description": "MagajiCo Sports Platform - AI-Powered Sports Predictions & Analytics",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "next dev -p 5000 --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start -p 5000 --hostname 0.0.0.0",
    "lint": "next lint",
    "api:dev": "python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload",
    "api:start": "python -m uvicorn main:app --host 0.0.0.0 --port 8000",
    "deploy:build": "npm run build",
    "deploy:start": "npm run start"
  },
  "keywords": [
    "sports",
    "api",
    "nfl",
    "nba",
    "mlb",
    "soccer",
    "odds",
    "live-scores"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^24.9.2",
    "@vercel/analytics": "^1.5.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.552.0",
    "next": "^14.2.0",
    "next-intl": "^4.5.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/react": "19.2.2",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17"
  }
}
